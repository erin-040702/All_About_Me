import streamlit as st
import pandas as pd

st.title('''
  :red[_Welcome_] :orange[to] :gray[All] :green[About] :blue[Erin] :violet[Escobar] :bear:''')
st.markdown("Happy Spring &mdash;\
            :tulip::cherry_blossom::rose::hibiscus::sunflower::blossom:")
st.header('Metadata Expert. Librarian. Coder. Artist.', divider='gray')
st.subheader('Facts & Figures :abacus:')
st.write(pd.DataFrame({
  'Job Title': ['Metadata Analyst', 'Digital Asset Manager', 'Metadata Assistant', 'Reference Assistant'],
  'Relevant Skill': ['Streamlit Python', 'Business Interviews', 'Taxonomy', 'Query Assistance']
}))
st.caption('This DataFrame briefly records relevant experience, see :rainbow[resume] for more.')


