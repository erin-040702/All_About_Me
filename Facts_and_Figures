st.write(pd.DataFrame({
  'Job Title': ['Metadata Analyst', 'Digital Asset Manager', 'Metadata Assistant', 'Reference Assistant'],
  'Relevant Skill': ['Streamlit Python', 'Business Interviews', 'Taxonomy', 'Query Assistance']
}))

st.write('Sudowrite + authors = more books :book: on my shelf.')
